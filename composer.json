{
    "name": "gintonicweb/websockets",
    "description": "Websockets plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "voryx/thruway":"0.3.*",
        "firebase/php-jwt": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Websockets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Websockets\\Test\\": "tests"
        }
    },
    "bin": [
        "bin/wsserver"
    ]
}
